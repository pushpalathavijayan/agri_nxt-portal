# AgRO NXT Backend Configuration
# Copy this file to .env and update values as needed

# Server Port
PORT=5000

# MongoDB Connection String
# For local MongoDB:
MONGODB_URI=mongodb://localhost:27017/agro-nxt
# For MongoDB Atlas (cloud):
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/agro-nxt

# JWT Secret Key (change this in production!)
JWT_SECRET=your_jwt_secret_key_change_in_production_12345

# Environment
NODE_ENV=development

# Add more configuration as needed
# CORS_ORIGIN=http://localhost:3000
# LOG_LEVEL=debug
